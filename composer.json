{
    "name": "vaimo/binary-chromedriver",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Utility for installing Chrome Driver, whatever platform you are on, for use with Behat",
    "authors": [
        {
            "name": "Laurent Baey",
            "email": "laurent.baey@gmail.com"
        },
        {
            "name": "Allan Paiste",
            "email": "allan.paiste@vaimo.com"
        }
    ],
    "support": {
        "source": "https://github.com/vaimo/chromedriver",
        "docs": "https://github.com/vaimo/chromedriver",
        "issues": "https://github.com/vaimo/chromedriver/issues"
    },
    "require": {
        "ext-zip": "*",
        "composer-plugin-api": "^1.0"
    },
    "keywords": [
        "chromedriver",
        "acceptance testing",
        "browser testing",
        "downloader"
    ],
    "autoload": {
        "psr-4": {
            "Lbaey\\ChromeDriver\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "class": "Lbaey\\ChromeDriver\\Plugin"
    }
}
